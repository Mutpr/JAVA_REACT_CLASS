package week1_0328;

public class star {
	public static void main(String [] args) {
		for(int i=5;i>0; i--) {
			for(int j=0; j<i; j++) {
				System.out.print("*");
			}System.out.println();
		for(int k=0; k<5; k++) {
			for(int l=0; l<k; l++) {

			for(int s=5; s>k; s--) {
				System.out.print("*");
			}
		}

		}
		}
}

