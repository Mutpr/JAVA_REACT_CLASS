package JDBC_02_Contact;

public class ContactDTO {
	private String name;
	private String phone;
	
	public ContactDTO() {
		super();
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
}
